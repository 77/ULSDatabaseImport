<?xml version="1.0" encoding="utf-8" ?>
<!--  This file represents my DataFormat for error logging. The
      The error log will be named Profile_#_Errorlog.xml  One will
      be created for each ProfileName.
-->
<errorloggingreport>
   <profile>
      <profilenumber>1</profilenumber>
      <errorrpt>
         <index>
            <datetime>03 Sept 2002 - 5:00p</datetime>
            <destination>
               <location>\\NetWorkComputer\</location>
               <path>c$\</path>
               <errorsfoundinpush>
                  <sourcepath>d:\myDir\myData.dat</sourcepath>
                  <sourcepath>d:\myDirAlso\ThisFile.txt</sourcepath>
                  <sourcepath>\\ANetLocation\OtherDir\DataHere.dat</sourcepath>
               </errorsfoundinpush>
            </destination>
         </index>
         <index>
            <datetime>04 Sept 2002 - 10:15p</datetime>
            <destination>
               <location>c:\</location>
               <errorsfoundinpush>
                  <sourcepath>z:\DataPath\DataPath2\MyData.file</sourcepath>
                  <sourcepath>z:\DataPath\DataPath2\DP3\SomeFile.jpg</sourcepath>
               </errorsfoundinpush>
            </destination>
         </index>
      </errorrpt>
   </profile>
</errorloggingreport>
