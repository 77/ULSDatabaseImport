<?xml version="1.0" encoding="utf-8" ?>
<!--  This file represents my DataFormat for the file queue.
      If the pushsubdirs flag is true and a path ends with just a 
      directory it will only push the files found in that current
      directory level nothing beyond that point will be pushed.
-->
<jobqueuelist>
   <profile>
      <profilenumber>1</profilenumber>
      <queue>
         <index>
            <datetime>03 Sept 2002 - 5:00p</datetime>
            <filesinqueue>
               <sourcepath>d:\myPathtoPush</sourcepath>
               <sourcepath>d:\myDir\myData.dat</sourcepath>
               <sourcepath>d:\myDirAlso\ThisFile.txt</sourcepath>
               <sourcepath>\\ANetLocation\OtherDir\DataHere.dat</sourcepath>
            </filesinqueue>
            <pushsubdirs>true</pushsubdirs>
         </index>
         <index>
            <datetime>04 Sept 2002 - 10:15p</datetime>
            <filesinqueue>
               <sourcepath>z:\DataPath\DataPath2\MyData.file</sourcepath>
               <sourcepath>z:\DataPath\DataPath2\MyData.txt</sourcepath>
               <sourcepath>z:\DataPath\DataPath2\DP3\SomeFile.jpg</sourcepath>
            </filesinqueue>
            <pushsubdirs>false</pushsubdirs>
         </index>
      </queue>
   </profile>
</jobqueuelist>